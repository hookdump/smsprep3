<div class="row-fluid">

	<h2>Upload HTML table</h2>

	<form role="form" method="POST" action="/admin/content/upload" enctype="multipart/form-data">
		<div class="form-group">
			<label for="uploadFile">File</label>
			<input type="file" id="uploadFile" name="uploadFile">
			<p class="help-block">This should be a Google Spreadsheet exported to HTML</p>
		</div>
		<button type="submit" class="btn btn-default">Upload</button>
	</form>

</div>


<div class="row-fluid">
 	<h1>Import Content</h1>
	<ul>
	<% for(var x=0; x<files.length; x++) { %>
		<% var filename = files[x] %>
		<% // filename = filename.replace(".html", ".web"); %>
		<li>
			<a href='/admin/content/import/<%=filename%>'><%=filename%></a>
		</li>
	<% } %>
	</ul> 
</div>