<ul class="hidden templateItems">
  <!-- Chat by us. Use the class "by-me". -->
  <li class="by-me">
    <!-- Use the class "pull-left" in avatar -->
    <div class="avatar pull-left">
    <img src="/public/img/user_smsprep.jpg" alt=""/>
    </div>

    <div class="chat-content">
    <!-- In meta area, first include "name" and then "time" -->
    <div class="chat-meta"><span class='time-ago'>X</span> <span class="pull-right to-number">#123123123</span></div>
    <span class='body'></span>
    <div class="clearfix"></div>
    </div>
  </li> 

  <!-- Chat by other. Use the class "by-other". -->
  <li class="by-other">
    <!-- Use the class "pull-right" in avatar -->
    <div class="avatar pull-right">
    <img src="/public/img/user_student.jpg" alt=""/>
    </div>

    <div class="chat-content">
    <!-- In the chat meta, first include "time" then "name" -->
    <div class="chat-meta"><span class='time-ago'>X</span> <span class="pull-right from-number">#12223334444</span></div>
    <span class='body'></span>
    <div class="clearfix"></div>
    </div>
  </li>  
</ul>

<!-- Chats, File upload and Recent Comments -->
<div class="row-fluid">


  <div class="span6">
    
    <!-- Chat Widget -->
    <div class="widget wgreen">
      <!-- Widget title -->
      <div class="widget-head">
        <div class="pull-left">Messages</div>
        <div class="clearfix"></div>
      </div>

      <div class="widget-content">
        <!-- Widget content -->
        <div class="padd">
          
          <ul class="chats logMessages">

 

          </ul>

        </div>
      </div>


    </div>

  </div>






  <div class="span6">
    
    <!-- Chat Widget -->
    <div class="widget wgreen">
      <!-- Widget title -->
      <div class="widget-head">
        <div class="pull-left">Testing</div>
        <div class="widget-icons pull-right">
          <a href="#" class="wconfig"><i class="icon-cog"></i></a>
        </div>
        <div class="clearfix"></div>
      </div>

      <div class="widget-content">

        <div class='emulateLoading'>Loading...</div>

        <!-- Widget content -->
        <div class="padd">
          
          <ul class="chats logTesting">

                                                              

          </ul>

        </div>
      </div>


        <!-- Widget footer -->
        <div class="widget-foot">
            
          <!-- Chat input box -->
            <form class="form-inline">
              <div class="input-append row-fluid">
                <div class="testingConfig">
                  <input type="text" class="span4 testingConfig_phone" placeholder="Phone Number" value="90001110000">
                  <button class="btn btn-info testingConfig_start">Start testing</button>
                </div>

                <div class="testingMessage" style='display: none'>
                  <span class="pull-right testPhoneContainer">Sending from:<br /><span class='testPhone'></span></span>
                  <input type="text" class="span4 testingMessage_msg" placeholder="Message">
                  <button class="btn btn-info testingMessage_send">Send</button>
                </div>
              </div>              
            </form>

        </div>

    </div>

  </div>










</div>

<div id='messages_page'></div>

<script src="/public/js/_messages.js"></script>