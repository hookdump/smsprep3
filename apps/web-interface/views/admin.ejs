
<!-- Today status. jQuery Sparkline plugin used. -->

<div class="row-fluid">
  <div class="span12"> 
    <!-- List starts -->
    <ul class="today-datas">

      <!-- List #1 -->

      <li class="bred">
        <!-- Graph -->
        <div class="pull-left"><i class="icon-group"></i></div>
        <!-- Text -->
        <div class="datas-text pull-right"><span class="bold"><%=info.students_new%></span> New Today</div>
        <div class="datas-text pull-right datas-margin"><span class="bold"><%=info.students_active%></span> Active Today</div>
        <div class="datas-text pull-right datas-margin"><span class="bold"><%=info.students_total%></span> Students</div>

        <div class="clearfix"></div>
      </li>


      <li class="bgreen">
        <!-- Graph -->
        <div class="pull-left"><i class="icon-envelope"></i></div>
        <!-- Text -->
        <div class="datas-text pull-right"><span class="bold"><%=info.q_answered_percent%>%</span> Msgs Delivered</div>
        <div class="datas-text pull-right datas-margin"><span class="bold"><%=info.q_delivered%></span> Q's Delivered</div>

        <div class="clearfix"></div>
      </li>

      <li class="bviolet">
        <!-- Graph -->
        <div class="pull-left"><i class="icon-book"></i></div>
        <!-- Text -->
        <div class="datas-text pull-right"><span class="bold"><%=info.q_count%></span> Questions</div>

        <div class="clearfix"></div>
      </li> 

      <li class="blightblue">
        <!-- Graph -->
        <div class="pull-left"><i class="icon-time"></i></div>
        <!-- Text -->
        <div class="datas-text pull-right"><span class="bold"><%=info.crons_delivered%></span> Crons Today</div>

        <div class="clearfix"></div>
      </li>

    </ul> 
  </div>
</div>

<!-- Today status ends -->

<!-- Dashboard Graph starts -->

<div class="row-fluid">
  <div class="span8">

    <!-- Widget -->
    <div class="widget wgreen">
      <!-- Widget head -->
      <div class="widget-head">
        <div class="pull-left">Questions Delivered</div>
        <div class="widget-icons pull-right">
          <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a> 
          <a href="#" class="wclose"><i class="icon-remove"></i></a>
        </div>
        <div class="clearfix"></div>
      </div>             

      <!-- Widget content -->
      <div class="widget-content">
        <div class="padd">

          <!-- Bar chart (Blue color). jQuery Flot plugin used. -->
          <div id="bar-chart"></div>


        </div>
      </div>
      <!-- Widget ends -->

    </div>
  </div>

  <div class="span4">

    <div class="widget wblue">

      <div class="widget-head">
        <div class="pull-left">Today Stats</div>
        <div class="widget-icons pull-right">
          <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a> 
          <a href="#" class="wclose"><i class="icon-remove"></i></a>
        </div>
        <div class="clearfix"></div>
      </div>             

      <div class="widget-content">
        <div class="padd">

          <!-- Visitors, pageview, bounce rate, etc., Sparklines plugin used -->
          <ul class="current-status">
            <li>
              <span id="status4"></span> <span class="bold">New Students : <b>0</b></span>
            </li>
            <li>
              <span id="status5"></span> <span class="bold">Questions Delivered : <b>0</b></span>
            </li>
            <li>
              <span id="status6"></span> <span class="bold">Questions Answered : <b>0%</b></span>
            </li>   
            <li>
              <span id="status7"></span> <span class="bold">Opt-outs : <b>0</b></span>
            </li>                                                                                                            
          </ul>

        </div>
      </div>

    </div>




    <div class="widget wblue">

      <div class="widget-head">
        <div class="pull-left">This Month Stats</div>
        <div class="widget-icons pull-right">
          <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a> 
          <a href="#" class="wclose"><i class="icon-remove"></i></a>
        </div>
        <div class="clearfix"></div>
      </div>             

      <div class="widget-content">
        <div class="padd">

          <!--
          <i class="icon-arrow-down red">
          <i class="icon-arrow-up green">
          -->

          <!-- Visitors, pageview, bounce rate, etc., Sparklines plugin used -->
          <ul class="current-status">
            <li>
              <span id="status4"></span> <span class="bold">New Students : <b>0</b></span>
            </li>
            <li>
              <span id="status5"></span> <span class="bold">Questions Delivered : <b>0</b></span>
            </li>
            <li>
              <span id="status6"></span> <span class="bold">Questions Answered : <b>0%</b></span>
            </li>   
            <li>
              <span id="status7"></span> <span class="bold">Opt-outs : <b>0</b></span>
            </li>                                                                                                            
          </ul>

        </div>
      </div>

    </div>









  </div>
</div>
<!-- Dashboard graph ends -->

